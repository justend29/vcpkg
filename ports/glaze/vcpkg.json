{
  "name": "glaze",
  "version": "1.2.1",
  "port-version": 0,
  "description": "Extremely fast, in memory, JSON and interface library for modern C++",
  "homepage": "https://github.com/stephenberry/glaze",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "eigen": {
      "description": "Supports reading and writing from Eigen Vector types",
      "dependencies": [
        "eigen3"
      ]
    },
    "jsonrpc": {
      "description": "Glaze wrapper supporting JSON-RPC 2.0 specification."
    }
  }
}
